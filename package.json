{
    "name": "ddd-sample",
    "version": "0.0.1",
    "description": "Domain Driven Design Sample using Node JS",
    "main": "src/index.ts",
    "scripts": {
        "start": "nodemon -e js,ts,json ./src/index.ts --exec babel-node --inspect=5959 --ignore -w --extensions \\\".ts,.js\\\" ./src/index.ts",
        "typecheck": "tsc",
        "node:start": "node src/",
        "test": "jest src",
        "coverage": "jest --collectCoverageFrom=src/**.js --coverage src"
    },
    "license": "MIT",
    "dependencies": {
        "uuid": "^8.3.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.12.8",
        "@babel/core": "^7.12.9",
        "@babel/node": "^7.12.6",
        "@babel/plugin-proposal-class-properties": "^7.12.1",
        "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
        "@babel/preset-env": "^7.12.7",
        "@babel/preset-typescript": "^7.12.7",
        "@babel/plugin-transform-runtime": "^7.12.1",
        "@types/node": "^14.14.10",
        "@types/jest": "22.2.3",
        "@types/uuid": "^8.3.0",
        "jest": "^26.6.3",
        "nodemon": "^2.0.6",
        "typescript": "^4.1.2"
    }
}
